    <script>
        let navExpanded = false;
        const btn = document.getElementById('test-btn');
        if (btn){
            const navbar = document.getElementById('navbar-default');
            btn.addEventListener('click', function () {
                if (navExpanded) {
                    navbar.classList.add('tw-hidden');
                    navExpanded = false;
                }
                else{
                    navbar.classList.remove('tw-hidden');
                    navExpanded = true;
                }
            
            });
        }

        const closeSidebar = document.getElementById('close-sidebar');
        const openSidebar = document.getElementById('open-sidebar');
        const sidebar = document.getElementById('default-sidebar');
        if (sidebar) {
            closeSidebar.addEventListener('click', function () {
                sidebar.classList.add('tw-hidden');
                openSidebar.classList.remove('tw-hidden');
            });
            openSidebar.addEventListener('click', function () {
                openSidebar.classList.add('tw-hidden');
                sidebar.classList.remove('tw-hidden');
            });
        }
    </script>
